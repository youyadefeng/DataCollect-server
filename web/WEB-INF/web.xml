<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <!-- user -->
    <servlet>
        <servlet-name>LogOnServlet</servlet-name>
        <servlet-class>Servlet.User.LogOnServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>LogOnServlet</servlet-name>
        <url-pattern>/user/logon</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>Servlet.User.RegisterServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/user/register</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserInfoServlet</servlet-name>
        <servlet-class>Servlet.User.UserInfoServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>UserInfoServlet</servlet-name>
        <url-pattern>/user/userinfo</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SignInServlet</servlet-name>
        <servlet-class>Servlet.User.SignInServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SignInServlet</servlet-name>
        <url-pattern>/user/signin</url-pattern>
    </servlet-mapping>

    <!--Quest-->
    <servlet>
        <servlet-name>QuestServlet</servlet-name>
        <servlet-class>Servlet.Quest.GetQuestServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>QuestServlet</servlet-name>
        <url-pattern>/quest/allquest</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>QuestNotDoServlet</servlet-name>
        <servlet-class>Servlet.Quest.QuestNotDoServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>QuestNotDoServlet</servlet-name>
        <url-pattern>/quest/notdo</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>QuestPurseServlet</servlet-name>
        <servlet-class>Servlet.Quest.QuestPurseServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>QuestPurseServlet</servlet-name>
        <url-pattern>/quest/purse</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>QuestHavingServlet</servlet-name>
        <servlet-class>Servlet.Quest.QuestHavingServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>QuestHavingServlet</servlet-name>
        <url-pattern>/quest/having</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>QuestBuyServlet</servlet-name>
        <servlet-class>Servlet.Quest.UserBuyQuestServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>QuestBuyServlet</servlet-name>
        <url-pattern>/quest/buy</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetQuestionServlet</servlet-name>
        <servlet-class>Servlet.Quest.GetSubQuestionServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GetQuestionServlet</servlet-name>
        <url-pattern>/quest/getquestion</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetOptionServlet</servlet-name>
        <servlet-class>Servlet.Quest.GetOptionsServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GetOptionServlet</servlet-name>
        <url-pattern>/quest/getoption</url-pattern>
    </servlet-mapping>

    <!--Data-->
    <servlet>
        <servlet-name>GetDataServlet</servlet-name>
        <servlet-class>Servlet.Data.GetDataServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GetDataServlet</servlet-name>
        <url-pattern>/data/get</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>InsertDataServlet</servlet-name>
        <servlet-class>Servlet.Data.InsertDataServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>InsertDataServlet</servlet-name>
        <url-pattern>/data/insert</url-pattern>
    </servlet-mapping>

</web-app>